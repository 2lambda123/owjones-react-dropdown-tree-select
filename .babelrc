{
  "presets": [["es2015", { "modules": false }], "stage-0", "react"],
  "plugins": ["transform-class-properties"],
  "env": {
    "production": {
      "plugins": ["transform-react-remove-prop-types"]
    },
    "test": {
      "presets": ["es2015", "stage-0", "react"],
      "plugins": [
        "istanbul",
        [
          "transform-runtime",
          {
            "polyfill": false,
            "regenerator": true
          }
        ]
      ]
    }
  }
}
